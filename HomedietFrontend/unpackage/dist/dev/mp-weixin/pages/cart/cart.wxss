.blank {
  height: 100px;
}
.fixbar {
  display: flex;
  z-index: 1;
  position: fixed;
  top: 990rpx;
  width: 375px;
  height: 60px;
  background-color: white;
  border-color: rgba(0, 0, 0, 0.04);
  border-width: 1px;
  border-style: solid;
}
.fixbar checkbox-group {
  margin-top: 30rpx;
  margin-left: 20rpx;
}
.fixbar label:nth-child(2) {
  margin-top: 35rpx;
  margin-left: 20rpx;
  font-family: SourceHanSansSC;
  font-weight: 400;
  font-size: 14px;
  color: #909090;
  font-style: normal;
  letter-spacing: 0px;
  line-height: 20px;
  text-decoration: none;
}
.fixbar label:nth-child(3) {
  margin-top: 35rpx;
  margin-left: 40rpx;
  font-family: SourceHanSansSC;
  font-weight: 400;
  font-size: 14px;
  color: rgba(255, 0, 0, 0.7);
  font-style: normal;
  letter-spacing: 0px;
  line-height: 20px;
  text-decoration: none;
}
.fixbar label:nth-child(4) {
  margin-top: 35rpx;
  margin-left: 40rpx;
  width: 42px;
  height: 20px;
  color: #101010;
  font-size: 14px;
  text-align: left;
  font-family: SourceHanSansSC-bold;
}
.fixbar label:nth-child(5) {
  margin-top: 32rpx;
  margin-left: 0rpx;
  width: auto;
  height: 22px;
  color: #0050b3;
  font-size: 16px;
  text-align: left;
  font-family: PingFangSC-bold;
}
.fixbar button {
  position: fixed;
  top: 1004rpx;
  left: 520rpx;
  width: 110px;
  height: 40px;
  line-height: 40px;
  border-radius: 100px;
  background-color: #0050b3;
  color: white;
  font-size: 14px;
  text-align: center;
  font-family: Arial;
  border: 1px solid #0050b3;
}
.goods {
  margin-bottom: 10rpx;
}
.goods .goodsCard {
  display: flex;
  position: relative;
  margin: 10rpx 0 10rpx 0;
  height: 256rpx;
  background-color: white;
  border-style: none;
  border-color: unset;
  box-shadow: rgba(0, 0, 0, 0.12) 0px 2px 6px 0px;
  /* color: rgb(255, 255, 255); */
  border-radius: 8px 12px 12px;
}
.goods .goodsCard checkbox-group {
  margin-top: 100rpx;
  margin-left: 22rpx;
}
.goods .goodsCard image {
  display: block;
  margin-left: 0px;
  margin-right: 8px;
  margin-top: 22px;
  width: 100px;
  height: 100px;
  border-radius: 14px;
  vertical-align: middle;
}
.goods .goodsCard view {
  margin-top: 5px;
  width: 90%;
  /* height: 80%; */
  /* 商品简介 */
  /* 商品价格  numberbox*/
}
.goods .goodsCard view view:nth-child(1) {
  height: 40%;
  font-family: PingFangSC;
  font-weight: 700;
  font-size: 14px;
  color: #101010;
  font-style: normal;
  letter-spacing: 0px;
  line-height: 20px;
  text-decoration: none;
  margin-left: 10rpx;
  /* padding-bottom: 3px; */
}
.goods .goodsCard view view:nth-child(2) {
  position: relative;
  display: flex;
  height: 50%;
}
.goods .goodsCard view view:nth-child(2) text:nth-child(1) {
  font-family: PingFangSC;
  font-weight: 700;
  font-size: 16px;
  font-style: normal;
  letter-spacing: 0px;
  line-height: 22px;
  text-decoration: none;
  /* 						margin: 0 10rpx 0 10rpx;
						margin-top: -13rpx; */
  position: absolute;
  bottom: 5px;
  color: #0050b3;
  /* 	justify-content: space-between;
						align-items: center; */
}
.goods .goodsCard view view:nth-child(2) uni-number-box {
  position: absolute;
  bottom: 5px;
  right: 8px;
  margin-top: 0;
  width: 50%;
